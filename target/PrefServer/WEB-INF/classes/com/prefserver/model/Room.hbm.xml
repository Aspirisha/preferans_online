<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.prefserver.model.Room" table="rooms">
      <meta attribute="class-description">
         This class contains the Room detail. 
      </meta>
      <id name="id" type="long" column="id">
         <generator class="native"/>
      </id>
      <property name="name" column="name" type="string"/>
      <property name="password" column="password" type="string"/>
      <property name="bullet" column="bullet" type="int"/> 
      <property name="playersNumber" column="players_number" type="byte"/>
      <property name="gameType" column="game_type" type="byte"/>
      <property name="whistCost" column="whist_cost" type="float"/> 
      <property name="raspExit" column="rasp_exit" type="string"/> 
      <property name="raspProgression" column="rasp_progression" type="string"/> 
      <property name="withoutThree" column="without_three" type="boolean"/> 
      <property name="noWhistRaspExit" column="no_whist_rasp_exit" type="boolean"/> 
      <property name="stalingrad" column="stalingrad" type="boolean"/> 
      <property name="tenWhist" column="ten_whist" type="boolean"/> 
   </class>
</hibernate-mapping>